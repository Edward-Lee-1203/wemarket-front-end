<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/image_dialog_food_image"
        android:layout_width="match_parent"
        android:layout_height="@dimen/WeMarket.size.coverPhoto"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <include
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/image_dialog_food_image"
        app:layout_constraintBottom_toTopOf="@+id/image_dialog_food_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/WeMarket.size.coverPhoto.oneThird"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.9"
        layout="@layout/include_dialog_food_details"
        />



</androidx.constraintlayout.widget.ConstraintLayout>